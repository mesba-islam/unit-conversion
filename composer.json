{
    "name": "mesba/unit-sonversion",
    "description": "perform unit conversion in php",
    "keywords": [
        "mesba",
        "unit conversions"
    ],
    "homepage": "https://github.com/:vendor_slug/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": "mesba",
            "email": "mesbansu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "mesba\\unit-conversion\\": "src
            "
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mesba\\unit-conversion\\Tests\\
            ": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "codestyle": "phpcs"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpunit/phpunit": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
